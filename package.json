{
  "dependencies": {
    "csstree": "csstree/csstree"
  },
  "scripts": {
    "build": "cd node_modules/css-tree && npm run build && cd ../.. && cp -f node_modules/css-tree/dist/csstree.js ./docs"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    },
    "rules": {
      "no-duplicate-case": 2,
      "no-undef": 2,
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "args": "after-used"
        }
      ]
    }
  }
}
